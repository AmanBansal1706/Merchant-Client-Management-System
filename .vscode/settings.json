{
  "explorer.confirmDelete": false,
  "window.zoomLevel": 1,
  "workbench.editor.enablePreview": false,
  "editor.tabSize": 2,
  "liveServer.settings.donotShowInfoMsg": true,
  "redhat.telemetry.enabled": true,
  "git.enableSmartCommit": true,
  "liveServer.settings.donotVerifyTags": true,
  "editor.wordWrap": "on",
  "git.openRepositoryInParentFolders": "never",
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "workbench.iconTheme": "symbols",
  "files.autoSave": "onFocusChange",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.run": "onSave",
  "scm.diffDecorations": "none",
  "powermode.enabled": true,
  "powermode.shake.intensity": 2,
  "powermode.explosions.frequency": 1,
  "powermode.explosions.maxExplosions": 3,
  "powermode.combo.counterSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "material-ui-snippets.showNotesOnStartup": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "git.autofetch": true,
  "githubPullRequests.pullBranch": "never",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorTheme": "Black Iris"
}
